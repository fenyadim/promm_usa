{
	"trailingComma": "es5",
	"tabWidth": 2,
	"useTabs": true,
	"semi": false,
	"singleQuote": true,
	"importOrder": [
		"<THIRD_PARTY_MODULES>",
		"^@/components$",
		"^@/pages$",
		"^@/redux/(.*)$",
		"^@/constans/(.*)$",
		"^@/services/(.*)$",
		"^@/utils/(.*)$",
		"^@/types/(.*)$",
		"^@/types$",
		"^@/api/(.*)$",
		"^../(.*)",
		"^./(.*)",
		"(.scss)$"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
